package looptask5421;

public class Starpattern {
	public static void main(String[] args) {
		
		
		for(int i=1;i<=5;i++){
			for(int k=1;k<=i;k++){
				System.out.print(" ");
			}
			for(int j=5;j>=i;j--){
				System.out.print("*"+" ");
			}
			System.out.println();
		}
		
		char a='E';
		for(int i=1;i<=5;i++){
			for(int k=1;k<=i;k++){
				System.out.print(" ");
			}
			for(int j=5;j>=i;j--){
				System.out.print(a+" ");
			}
			System.out.println();
			a--;
		}
		
		  int b=5;
		for(int i=1;i<=5;i++){
			for(int k=1;k<=i;k++){
				System.out.print(" ");
			}
			for(int j=5;j>=i;j--){
				System.out.print(b+" ");
			}
			System.out.println();
	         b=b-1;
		}
		
		char ch='E';
		for(int i=1;i<=5;i++){
			for(int k=1;k<=i;k++){
				System.out.print(" ");
			}
			for(int j=5;j>=i;j--){
				System.out.print("*"+" ");
			}
			System.out.println();
		int c=(char)ch-1;
		}
		
	}

}
